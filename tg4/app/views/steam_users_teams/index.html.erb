<h1>Listing steam_users_teams</h1>

<table>
  <thead>
    <tr>
      <th>Steam user</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @steam_users_teams.each do |steam_users_team| %>
      <tr>
        <td><%= steam_users_team.steam_user_id %></td>
        <td><%= steam_users_team.team_id %></td>
        <td><%= link_to 'Show', steam_users_team %></td>
        <td><%= link_to 'Edit', edit_steam_users_team_path(steam_users_team) %></td>
        <td><%= link_to 'Destroy', steam_users_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Steam users team', new_steam_users_team_path %>
